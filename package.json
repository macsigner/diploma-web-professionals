{
    "name": "home-house",
    "version": "1.0.0",
    "description": "Diplomarbeit 2022 Web Frontend Developer",
    "type": "module",
    "scripts": {
        "start": "webpack-dev-server --open --config webpack.dev.js",
        "watch:doc:frontend": "npx nodemon --exec 'npm run build:doc:frontend' --watch src",
        "build": "webpack --config webpack.prod.js",
        "build:dev": "webpack --config webpack.dev.js",
        "build:doc": "npm run build:doc:frontend && npm run build:doc:backend",
        "build:doc:backend": "jsdoc -p -r node-scripts -d jsdoc-backend",
        "build:doc:frontend": "jsdoc -p -r src/js -d jsdoc-frontend",
        "deploy": "npm run build && npm run ghpages",
        "deploy:doc": "npm run build:doc && npm run ghpages:doc",
        "ghpages": "gh-pages -d dist -b ghpages",
        "ghpages:doc": "gh-pages -d jsdoc-frontend -b jsdoc-frontend && gh-pages -d jsdoc-backend -b jsdoc-backend",
        "debug": "nodemon ./node_modules/ndb/ndb.js",
        "lint:scss": "stylelint 'src/scss/**/*.scss'",
        "lint:scss:fix": "stylelint --fix 'src/scss/**/*.scss'",
        "lint:js": "eslint 'src/js/**/*.js' 'node-scripts/**/*.js' './*.js'",
        "lint:js:fix": "eslint --fix 'src/js/**/*.js' 'node-scripts/**/*.js' './*.js'",
        "lint": "npm run lint:scss:fix && npm run lint:js:fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/macsigner/diploma-web-professionals.git"
    },
    "keywords": [],
    "author": "Mac Signer",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/macsigner/diploma-web-professionals/issues"
    },
    "homepage": "https://github.com/macsigner/diploma-web-professionals#readme",
    "devDependencies": {
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.7.1",
        "eslint": "^7.32.0",
        "eslint-loader": "^4.0.2",
        "eslint-webpack-plugin": "^3.2.0",
        "gh-pages": "^4.0.0",
        "glob": "^8.0.3",
        "graphql": "^16.6.0",
        "graphql-request": "^5.0.0",
        "html-loader": "^4.1.0",
        "html-webpack-plugin": "^5.5.0",
        "jsdoc": "^3.6.11",
        "mini-css-extract-plugin": "^2.6.1",
        "ndb": "^1.1.5",
        "nodemon": "^2.0.20",
        "normalize.css": "^8.0.1",
        "raw-loader": "^4.0.2",
        "sass": "^1.55.0",
        "sass-loader": "^13.0.2",
        "style-loader": "^3.3.1",
        "stylelint": "^14.13.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "stylelint-order": "^5.0.0",
        "stylelint-scss": "^4.3.0",
        "stylelint-webpack-plugin": "^3.3.0",
        "twig-html-loader": "^0.1.9",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0"
    }
}
